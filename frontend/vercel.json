{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"rewrites": [
		{
			"source": "/api/(.*)",
			"destination": "https://api.sleylanguren.com/$1"
		},
		{ "source": "/(.*)", "destination": "/" },
		{ "source": "/assets/(.*)", "destination": "/src/assets" }
	],
	"git": {
		"deploymentEnabled": false
	}
}
